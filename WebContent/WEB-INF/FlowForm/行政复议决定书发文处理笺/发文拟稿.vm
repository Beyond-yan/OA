	save : function() {
		Ext.getCmp('XZFYSentArchiveStart.sources').allowBlank = false;//行文方向
		Ext.getCmp('XZFYSentArchiveForm.flowAssignId').allowBlank = false;//核稿
		Ext.getCmp('XZFYSentArchiveForm.archives.sendTo').allowBlank = false;//主送
