<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.gdssoft.oa.model.system.SysUserAll"
		table="SYS_USER_ALL"
		select-before-update="false"
		dynamic-update="false"
		dynamic-insert="false"
		optimistic-lock="version"
		schema="OA_COMMON">
		<id
			name="id"
			column="ID"
			type="java.lang.Long"
			length="22"
			>
			<generator class="native"/>
		</id>
		<property
			name="userName"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="USER_NAME"
			length="64"
			not-null="false"
			unique="false"
			/>
		<property
			name="userPassword"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="USER_PASSWORD"
			length="64"
			not-null="false"
			unique="false"
			/>
		<property
			name="status"
			type="java.lang.Long"
			update="true"
			insert="true"
			access="property"
			column="STATUS"
			length="22"
			not-null="false"
			unique="false"
			/>
		<property
			name="createUser"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="CREATE_USER"
			length="64"
			not-null="false"
			unique="false"
			/>
		<property
			name="createDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="CREATE_DATE"
			length="7"
			not-null="false"
			unique="false"
			/>
		<property
			name="updateUser"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="UPDATE_USER"
			length="64"
			not-null="false"
			unique="false"
			/>
		<property
			name="updateDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="UPDATE_DATE"
			length="7"
			not-null="false"
			unique="false"
			/>
		<!-- imported bi-directional many-to-one association to SysSchemaConfig -->
		<many-to-one
			name="sysSchemaConfig"
			class="com.gdssoft.oa.model.system.SysSchemaConfig"
			not-null="false"
			fetch="select"
			>	
			<column name="SCHEMA_ID" />
		</many-to-one>
	</class>
</hibernate-mapping>
