<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.system.SysMessage"
        table="SYS_MESSAGE"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
       >
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="22"
        >
              <generator class="native"/>
        </id>
        <property
            name="subject"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SUBJECT"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="original_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="ORIGINAL_ID"
            length="22"
            not-null="false"
            unique="false"
        />
        <property
            name="runId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="RUNID"
            length="22"
            not-null="false"
            unique="false"
        />
        <property
            name="taskId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="TASKID"
            length="22"
            not-null="false"
            unique="false"
        />
        <property
            name="to_user"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="TO_USER"
            length="22"
            not-null="false"
            unique="false"
        />
     
        <property
            name="create_date"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="original_type"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="ORIGINAL_TYPE"
            length="3"
            not-null="false"
            unique="false"
        />
         <property
            name="read_flag"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="READ_FLAG"
            length="3"
            not-null="false"
            unique="false"
        />
    </class>
</hibernate-mapping>
