<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.system.SysArchivesFiles"
        table="SYS_ARCHIVES_FILES"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
     schema="OA_COMMON">
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="22"
        >
              <generator class="native"/>
        </id>
         <property
            name="fileType"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="FILE_TYPE"
            length="22"
            not-null="false"
            unique="false"
        />
        <property
            name="fileVersion"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="FILE_VERSION"
            length="22"
            not-null="false"
            unique="false"
        />
        <property
            name="fileName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="FILE_NAME"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="filePath"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="FILE_PATH"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="fileSize"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="FILE_SIZE"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="fileByteSize"
            type="java.lang.Double"
            update="true"
            insert="true"
            access="property"
            column="FILE_BYTE_SIZE"
            length="22"
            not-null="false"
            unique="false"
        />
        <property
            name="fileExtType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="FILE_EXT_TYPE"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="fileDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="FILE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
         <property
            name="isFinish"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="IS_FINISH"
            length="5"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to SysDataTransfer -->
		<many-to-one
			name="sysDataTransfer"
			class="com.gdssoft.oa.model.system.SysDataTransfer"
            not-null="false"
			fetch="select"
		>	
			<column name="DATA_ID" />
		</many-to-one>
    </class>
</hibernate-mapping>
