<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.system.SysServiceAccount"
        table="SYS_SERVICE_ACCOUNT"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
        schema="OA_COMMON">
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="22"
        >
              <generator class="native"/>
        </id>
         <property
            name="serviceAccount"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SERVICE_ACCOUNT"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="password"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="PASSWORD"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DESCRIPTION"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="STATUS"
            length="22"
            not-null="false"
            unique="false"
        />
        <property
            name="createUser"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CREATE_USER"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="updateUser"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_USER"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
  <!-- exported bi-directional one-to-many association to SysInterfaceAccount -->
	<set name="sysInterfaceAccounts"
	     table="sys_interface_account"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="ACCOUNT_ID"/>
		</key>
		<one-to-many
			class="com.gdssoft.oa.model.system.SysInterfaceAccount"
		/>
	</set>     
    </class>
</hibernate-mapping>
