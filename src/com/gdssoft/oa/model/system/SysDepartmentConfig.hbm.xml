<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.system.SysDepartmentConfig"
        table="SYS_DEPARTMENT_CONFIG"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
        schema="OA_COMMON">
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="22"
        >
              <generator class="native"/>
        </id>
         <property
            name="depCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DEP_CODE"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="depName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DEP_NAME"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="schemaId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="SCHEMA_ID"
            length="22"
            not-null="false"
            unique="false"
        />
        <property
            name="createUser"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CREATE_USER"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="updateUser"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_USER"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="depId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="DEP_ID"
            length="22"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
