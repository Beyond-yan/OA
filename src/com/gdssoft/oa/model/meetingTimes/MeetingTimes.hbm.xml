    <property name="times" type="java.lang.Long" update="true" insert="true" access="property" column="TIMES" length="4000" not-null="false" unique="false"/>  
    <property name="type" type="java.lang.Long" update="true" insert="true" access="property" column="TYPE" length="4000" not-null="false" unique="false"/>  
  </class> 
</hibernate-mapping>