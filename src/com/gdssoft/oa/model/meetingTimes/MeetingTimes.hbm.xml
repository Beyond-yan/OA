<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping> 
  <class name="com.gdssoft.oa.model.meetingTimes.MeetingTimes" table="MEETING_TIMES" select-before-update="false" dynamic-update="false" dynamic-insert="false" optimistic-lock="version"> 
    <id name="timesId" column="TIMESID" type="java.lang.Long"> 
      <generator class="native"></generator> 
    </id>  
