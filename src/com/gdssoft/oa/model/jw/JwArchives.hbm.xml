<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.jw.JwArchives"
        table="JW_ARCHIVES"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="22"
        >
              <generator class="native"/>
        </id>
         <property
            name="subject"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SUBJECT"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="sourcedepartment"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SOURCEDEPARTMENT"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="page"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="PAGE"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="docnum"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DOCNUM"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="burden"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="BURDEN"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="doctype"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DOCTYPE"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="securitydegree"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SECURITYDEGREE"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="day"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DAY"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="bumfyear"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="BUMFYEAR"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="keywords"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="KEYWORDS"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="attachcode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ATTACHCODE"
            length="500"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
