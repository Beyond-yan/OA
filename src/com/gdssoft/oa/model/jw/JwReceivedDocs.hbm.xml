            name="urgencydegree"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="URGENCYDEGREE"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="securitydegree"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SECURITYDEGREE"
            length="500"
            not-null="false"
            unique="false"
