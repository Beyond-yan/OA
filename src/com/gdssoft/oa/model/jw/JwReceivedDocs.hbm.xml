            name="unittitle"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UNITTITLE"
            length="3000"
            not-null="false"
            unique="false"
        />
        <property
            name="acceptno"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ACCEPTNO"
            length="1000"
            not-null="false"
            unique="false"
