            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="TOPIC"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="dept"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DEPT"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="planmemo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="PLANMEMO"
            length="2000"
            not-null="false"
            unique="false"
