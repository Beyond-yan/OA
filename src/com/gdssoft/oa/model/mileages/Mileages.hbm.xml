<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.gdssoft.oa.model.mileages.Mileages"
		table="car_mileages"
		select-before-update="false"
		dynamic-update="false"
		dynamic-insert="false"
		optimistic-lock="version"
		>
		<id
			name="id"
			column="ID"
			type="java.lang.Long"
			length="19"
			>
			<generator class="native"/>
		</id>
		<property
			name="travelDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="TRAVEL_DATE"
			length="50"
			not-null="false"
			unique="false"
			/>
		<property
			name="endNumber"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="END_NUMBER"
			length="50"
			not-null="false"
			unique="false"
			/>
			<property
			name="monthMileage"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="MONTH_MILEAGE"
			length="50"
			not-null="false"
			unique="false"
			/>
		<property
			name="createDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="CREATE_DATE"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="createUser"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="CREATE_USER"
			length="50"
			not-null="false"
			unique="false"
			/>
		<property
			name="updateDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="UPDATE_DATE"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="updateUser"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="UPDATE_User"
			length="50"
			not-null="false"
			unique="false"
			/>
			<many-to-one name="car" class="com.gdssoft.oa.model.admin.Car" not-null="false" fetch="select"> 
			<column name="CAR_ID"/> 
		</many-to-one> 
		<!-- exported bi-directional one-to-many association to CarUsing -->
	</class>
</hibernate-mapping>
