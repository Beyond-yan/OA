<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.snconfig.FileSnConfig"
        table="CQ_FILE_SN_CONFIG"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="22"
        >
              <generator class="native"/>
        </id>
         <property
            name="snName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SN_NAME"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="snNumber"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="SN_NUMBER"
            length="22"
            not-null="false"
            unique="false"
        />
        <property
            name="snFormat"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SN_FORMAT"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="snType"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="SN_TYPE"
            length="22"
            not-null="false"
            unique="false"
        />
        <property
            name="expirationDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="EXPIRATION_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="createUser"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CREATE_USER"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="updateUser"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_USER"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
  <!-- exported bi-directional one-to-many association to CqFlowSnConfig -->
	<set name="flowSnConfigs"
	     table="cq_flow_sn_config"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="SN_ID"/>
		</key>
		<one-to-many
			class="com.gdssoft.oa.model.snconfig.FlowSnConfig"
		/>
	</set>     
	<set name="fileSnConfigHistorys"
	     table="cq_flow_sn_config_history"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted">
		<key>
			<column name="SN_ID"/>
		</key>
		<one-to-many
			class="com.gdssoft.oa.model.snconfig.FileSnConfigHistory"
		/>
	</set> 
    </class>
</hibernate-mapping>
