<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.archive.OdCommonComments"
        table="od_common_comments"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="19"
        >
              <generator class="native"/>
        </id>
         <property
            name="commentTitle"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="COMMENT_TITLE"
            length="50"
            not-null="true"
            unique="false"
        />
        <property
            name="commentDesc"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="COMMENT_DESC"
            length="400"
            not-null="false"
            unique="false"
        />
        <property
            name="commentType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="COMMENT_TYPE"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="ref1"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="REF1"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="ref2"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="REF2"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="ref3"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="REF3"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CREATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to AppUser -->
		<many-to-one
			name="appUser"
			class="com.gdssoft.oa.model.system.AppUser"
			fetch="select"
			not-null="false"
			>	
			<column name="USER_ID" />
		</many-to-one>
    </class>
</hibernate-mapping>
