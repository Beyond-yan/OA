<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.personal.AddrbookOuter"
        table="ADDRBOOK_OUTER"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="19"
        >
              <generator class="native"/>
        </id>
         <property
            name="personName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="PERSON_NAME"
            length="50"
            not-null="true"
            unique="false"
        />
        <property
            name="personSex"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="PERSON_SEX"
            length="5"
            not-null="true"
            unique="false"
        />
        			<property
			name="email"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="EMAIL"
			length="50"
			not-null="false"
			unique="false"
			/>
			
        <property
            name="company"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="COMPANY"
            length="50"
            not-null="true"
            unique="false"
        />
        <property
            name="department"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DEPARTMENT"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="room"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ROOM"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="officePhone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="OFFICE_PHONE"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="ext"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="EXT"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="mobile"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="MOBILE"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="shortMobile"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SHORT_MOBILE"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CREATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
