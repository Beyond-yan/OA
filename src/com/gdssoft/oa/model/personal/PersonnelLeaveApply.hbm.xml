            name="type"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="TYPE"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="startDt"
            type="java.util.Date"
            update="true"
            insert="true"
