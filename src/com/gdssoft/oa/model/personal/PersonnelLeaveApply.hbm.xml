            access="property"
            column="END_DT"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="backDt"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="BACK_DT"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DESCRIPTION"
            length="400"
            not-null="false"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="REMARK"
            length="400"
            not-null="false"
            unique="false"
        />
        <property
            name="isDeleted"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="IS_DELETED"
            length="5"
            not-null="false"
            unique="false"
