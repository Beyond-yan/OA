<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.gdssoft.oa.model.personal.PersonnelEmployee"
		table="PERSONNEL_EMPLOYEE"
		select-before-update="false"
		dynamic-update="false"
		dynamic-insert="false"
		optimistic-lock="version"
		>
		<id
			name="id"
			column="ID"
			type="java.lang.Long"
			length="19"
			>
			<generator class="native"/>
		</id>
		<property
			name="empCode"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="EMP_CODE"
			length="30"
			not-null="true"
			unique="false"
			/>
		<property
			name="officePhone"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="OFFICE_PHONE"
			length="30"
			not-null="false"
			unique="false"
			/>
		<property
			name="shortPhone"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="SHORT_PHONE"
			length="30"
			not-null="false"
			unique="false"
			/>
		<property
			name="imageSource"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="IMAGE_SOURCE"
			length="150"
			not-null="false"
			unique="false"
			/>
		<property
			name="isLeaving"
			type="java.lang.Short"
			update="true"
			insert="true"
			access="property"
			column="IS_LEAVING"
			length="5"
			not-null="true"
			unique="false"
			/>
					<property
			name="isLeader"
			type="java.lang.Short"
			update="true"
			insert="true"
			access="property"
			column="IS_LEADER"
			length="5"
			not-null="false"
			unique="false"
			/>
					<property
			name="isWorktel"
			type="java.lang.Short"
			update="true"
			insert="true"
			access="property"
			column="IS_WORKTEL"
			length="5"
			not-null="false"
			unique="false"
			/>
		<property
			name="beforeSeniority"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="BEFORE_SENIORITY"
			precision="3"
			scale="1"
			not-null="false"
			unique="false"
			/>
		<property
			name="leaveDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="LEAVE_DATE"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="totalSeniority"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="TOTAL_SENIORITY"
			precision="3"
			scale="1"
			not-null="false"
			unique="false"
			/>
		<property
			name="remark"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="REMARK"
			length="400"
			not-null="false"
			unique="false"
			/>
		<property
			name="createDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="CREATE_DATE"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="createBy"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="CREATE_BY"
			length="50"
			not-null="false"
			unique="false"
			/>

			
		<property
			name="updateDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="UPDATE_DATE"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="updateBy"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="UPDATE_BY"
			length="50"
			not-null="false"
			unique="false"
			/>
		<property name="ext" type="java.lang.String" 	update="true" insert="true" access="property" column="EXT"
 		length="10" not-null="false" unique="false"
			/>
		<property
			name="room"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="ROOM"
			length="50"
			not-null="false"
			unique="false"
			/>
		
			<many-to-one name="appUser" class="com.gdssoft.oa.model.system.AppUser" not-null="false"
			fetch="select"
			>	
			<column name="USER_ID" />
		</many-to-one>
		
	</class>
</hibernate-mapping>
