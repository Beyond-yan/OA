<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.duty.DutySignRecord"
        table="CQ_DUTY_SIGN_RECORD"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="22"
        >
              <generator class="native"/>
        </id>
         <property
            name="signDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="SIGN_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="signType"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="SIGN_TYPE"
            length="22"
            not-null="false"
            unique="false"
        />
        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DESCRIPTION"
            length="800"
            not-null="false"
            unique="false"
        />
        <property
            name="createUser"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="CREATE_USER"
            length="22"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to AppUser -->
		<many-to-one
			name="appUser"
			class="com.gdssoft.oa.model.system.AppUser"
            not-null="false"
			fetch="select"
		>	
			<column name="DUTY_ID" />
		</many-to-one>
    </class>
</hibernate-mapping>
