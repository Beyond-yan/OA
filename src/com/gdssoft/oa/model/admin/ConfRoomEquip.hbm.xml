<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.admin.ConfRoomEquip"
        table="conf_room_equip"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="19"
        >
              <generator class="native"/>
        </id>
         <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="NAME"
            length="50"
            not-null="true"
            unique="false"
        />
        <property
            name="amount"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="AMOUNT"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DESCRIPTION"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CREATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to Boardroo -->
		<many-to-one
			name="Boardroo"
			class="com.gdssoft.oa.model.admin.Boardroo"
            not-null="false"
           
            fetch="select"
            >	
			<column name="ROOM_ID" />
		</many-to-one>
    </class>
</hibernate-mapping>
