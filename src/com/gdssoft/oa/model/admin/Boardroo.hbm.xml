<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.admin.Boardroo"
        table="boardroo"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="roomId"
            column="roomid"
            type="java.lang.Long"
            length="19"
        >
              <generator class="native"/>
        </id>
         <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CODE"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="roomname"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="roomname"
            length="50"
            not-null="true"
            unique="false"
        />
        <property
            name="roomdesc"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="roomdesc"
            length="400"
            not-null="false"
            unique="false"
        />
        <property
            name="containnum"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="containnum"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="location"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="LOCATION"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="projector"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="PROJECTOR"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="STATUS"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CREATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="telphone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="TELPHONE"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="roomType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ROOM_TYPE"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="manageOrg"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="MANAGE_ORG"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="roomComment"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ROOM_COMMENT"
            length="500"
            not-null="false"
            unique="false"
        />
  <!-- exported bi-directional one-to-many association to ConfRoomEquip -->
	<set name="confRoomEquips"
	     table="conf_room_equip"
	      lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="ROOM_ID"/>
		</key>
		<one-to-many
			class="com.gdssoft.oa.model.admin.ConfRoomEquip"
		/>
	</set>        
    </class>
</hibernate-mapping>
