<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.gdssoft.oa.model.admin.CarInsurance"
		table="car_insurance"
		select-before-update="false"
		dynamic-update="false"
		dynamic-insert="false"
		optimistic-lock="version"
		>
		<id
			name="id"
			column="ID"
			type="java.lang.Long"
			length="19"
			>
			<generator class="native"/>
		</id>
		<property
			name="carId"
			type="java.lang.Long"
			update="false"
			insert="false"
			access="property"
			column="CAR_ID"
			length="23"
			not-null="true"
			unique="false"
			/>
		<property
			name="buyDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="BUY_DATE"
			length="23"
			not-null="true"
			unique="false"
			/>
		<property
			name="insrSDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="INSR_S_DATE"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="insrEDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="INSR_E_DATE"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="insrCompany"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="INSR_COMPANY"
			length="100"
			not-null="false"
			unique="false"
			/>
		<property
			name="ref1"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="REF1"
			length="50"
			not-null="false"
			unique="false"
			/>
		<property
			name="ref2"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="REF2"
			length="50"
			not-null="false"
			unique="false"
			/>
		<property
			name="ref3"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="REF3"
			length="50"
			not-null="false"
			unique="false"
			/>
		<property
			name="createDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="CREATE_DATE"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="createBy"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="CREATE_BY"
			length="50"
			not-null="false"
			unique="false"
			/>
		<property
			name="updateDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="UPDATE_DATE"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="updateBy"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="UPDATE_BY"
			length="50"
			not-null="false"
			unique="false"
			/>
		<!-- imported bi-directional many-to-one association to Car -->
		<many-to-one
			name="car"
			class="com.gdssoft.oa.model.admin.Car"
			not-null="false"
			fetch="select"
			>	
			<column name="CAR_ID" />
		</many-to-one>
	</class>
</hibernate-mapping>
