<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.admin.CarOilCard"
        table="car_oil_card"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="19"
        >
              <generator class="native"/>
        </id>
         <property
            name="type"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="TYPE"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CODE"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="sn"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SN"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="remains"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="REMAINS"
            precision="18"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="oilCompany"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="OIL_COMPANY"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="buyDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="BUY_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="validSDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="VALID_S_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="validEDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="VALID_E_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="REMARK"
            length="400"
            not-null="false"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="STATUS"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CREATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="cost"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="COST"
            precision="10"
            scale="2"
            not-null="false"
            unique="false"
        />
  <!-- exported bi-directional one-to-many association to Car -->
	<set name="cars"
	     table="car"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="OIL_CARD_ID"/>
		</key>
		<one-to-many
			class="com.gdssoft.oa.model.admin.Car"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to CarUsing -->
	<set name="carUsings"
	     table="car_using"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="OIL_CARD_ID"/>
		</key>
		<one-to-many
			class="com.gdssoft.oa.model.admin.CarUsing"
		/>
	</set>      
    </class>
</hibernate-mapping>
