<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.gdssoft.oa.model.admin.Car"
		table="car"
		select-before-update="false"
		dynamic-update="false"
		dynamic-insert="false"
		optimistic-lock="version"
		>
		<id
			name="carid"
			column="carid"
			type="java.lang.Long"
			length="19"
			>
			<generator class="native"/>
		</id>
		<property
			name="carno"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="carno"
			length="50"
			not-null="true"
			unique="false"
			/>
		<property
			name="cartype"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="cartype"
			length="10"
			not-null="true"
			unique="false"
			/>
		<property
			name="engineno"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="engineno"
			length="100"
			not-null="false"
			unique="false"
			/>
		<property
			name="buyinsuretime"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="buyinsuretime"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="audittime"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="audittime"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="notes"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="notes"
			length="400"
			not-null="false"
			unique="false"
			/>
		<property
			name="factorymodel"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="factorymodel"
			length="5"
			not-null="false"
			unique="false"
			/>
		<property
			name="driver"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="driver"
			length="32"
			not-null="false"
			unique="false"
			/>
		<property
			name="buydate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="buydate"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="status"
			type="java.lang.Short"
			update="true"
			insert="true"
			access="property"
			column="status"
			length="5"
			not-null="true"
			unique="false"
			/>
		<property
			name="cartimage"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="cartimage"
			length="150"
			not-null="false"
			unique="false"
			/>
		<property
			name="purpose"
			type="java.lang.Short"
			update="true"
			insert="true"
			access="property"
			column="PURPOSE"
			length="5"
			not-null="false"
			unique="false"
			/>
		<property
			name="carClass"
			type="java.lang.Short"
			update="true"
			insert="true"
			access="property"
			column="CAR_CLASS"
			length="5"
			not-null="false"
			unique="false"
			/>
		<property
			name="passAmount"
			type="java.lang.Short"
			update="true"
			insert="true"
			access="property"
			column="PASS_AMOUNT"
			length="5"
			not-null="false"
			unique="false"
			/>
		<property
			name="belongDeptId"
			type="java.lang.Long"
			update="true"
			insert="true"
			access="property"
			column="BELONG_DEPT_ID"
			length="19"
			not-null="false"
			unique="false"
			/>
		<property
			name="usingStatus"
			type="java.lang.Short"
			update="true"
			insert="true"
			access="property"
			column="USING_STATUS"
			length="5"
			not-null="false"
			unique="false"
			/>
	    
		<property
			name="createDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="CREATE_DATE"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="createBy"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="CREATE_BY"
			length="50"
			not-null="false"
			unique="false"
			/>
		<property
			name="updateDate"
			type="java.util.Date"
			update="true"
			insert="true"
			access="property"
			column="UPDATE_DATE"
			length="23"
			not-null="false"
			unique="false"
			/>
		<property
			name="updateBy"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="UPDATE_BY"
			length="50"
			not-null="false"
			unique="false"
			/>
			<property
			name="distance"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="TOTAL_DISTANCE"
			length="50"
			not-null="false"
			unique="false"
			/><property
			name="engineSpec"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="ENGINE_SPEC"
			length="50"
			not-null="false"
			unique="false"
			/><property
			name="purchase"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="PURCHASE_AMOUNT"
			length="50"
			not-null="false"
			unique="false"
			/>
			<property
			name="isPublic"
			type="java.lang.String"
			update="true"
			insert="true"
			access="property"
			column="IS_PUBLIC"
			length="50"
			not-null="false"
			unique="false"
			/>
			<many-to-one
			name="department"
			class="com.gdssoft.oa.model.system.Department"
			not-null="false"
			fetch="select"
			>	
			<column name="CAR_OWNER" />
		</many-to-one>
		<!-- imported bi-directional many-to-one association to CarOilCard -->
		<many-to-one
			name="carOilCard"
			class="com.gdssoft.oa.model.admin.CarOilCard"
			not-null="false"
			fetch="select"
			>	
			<column name="OIL_CARD_ID" />
		</many-to-one>
		<!-- imported bi-directional many-to-one association to CarPassFeeCard -->
		<many-to-one
			name="carPassFeeCard"
			class="com.gdssoft.oa.model.admin.CarPassFeeCard"
			not-null="false"
			fetch="select"
			>	
			<column name="PAY_CARD_ID" />
		</many-to-one>
		<!-- exported bi-directional one-to-many association to CarApply -->
	
	
        
		<!-- exported bi-directional one-to-many association to CarInsurance -->
		<set name="carInsurances"
			table="car_insurance"
			lazy="true"
			inverse="true"
			cascade="save-update"
			sort="unsorted"
			>
			<key>
				<column name="CAR_ID"/>
			</key>
			<one-to-many
				class="com.gdssoft.oa.model.admin.CarInsurance"
				/>
		</set>     
		<!-- exported bi-directional one-to-many association to CarUsing -->
		<set name="carUsings"
			table="car_using"
			lazy="true"
			inverse="true"
			cascade="save-update"
			sort="unsorted"
			>
			<key>
				<column name="CAR_ID"/>
			</key>
			<one-to-many
				class="com.gdssoft.oa.model.admin.CarUsing"
				/>
		</set>     
		<!-- exported bi-directional one-to-many association to CartRepair -->
		<set name="cartRepairs"
			table="cart_repair"
			lazy="true"
			inverse="true"
			cascade="save-update"
			sort="unsorted"
			>
			<key>
				<column name="carid"/>
			</key>
			<one-to-many
				class="com.gdssoft.oa.model.admin.CartRepair"
				/>
		</set>     
	</class>
</hibernate-mapping>
