<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.admin.CarUsing"
        table="car_using"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="19"
        >
              <generator class="native"/>
        </id>
         <property
            name="leavingDt"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="LEAVING_DT"
            length="23"
            not-null="true"
            unique="false"
        />
        <property
            name="comingDt"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="COMING_DT"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="distance"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DISTANCE"
            precision="18"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="passFee"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="PASS_FEE"
            precision="18"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="parkFee"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="PARK_FEE"
            precision="18"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="otherFee"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="OTHER_FEE"
            precision="18"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="feeAmount"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="FEE_AMOUNT"
            precision="18"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="feeId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="FEE_ID"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CREATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
        
        <property
            name="driver"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DRIVER"
            length="50"
            not-null="false"
            unique="false"
        />
        
        <property
            name="des"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="EDS"
            length="200"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to Car -->
		<many-to-one
			name="car"
			class="com.gdssoft.oa.model.admin.Car"
            not-null="false"
			fetch="select"
		>	
			<column name="CAR_ID" />
		</many-to-one>
    <!-- imported bi-directional many-to-one association to CarApply -->
		<many-to-one
			name="carApply"
			class="com.gdssoft.oa.model.admin.CarApply"
            not-null="false"
			fetch="select"
		>	
			<column name="APPLY_ID" />
		</many-to-one>
    <!-- imported bi-directional many-to-one association to CarDriver -->
		<many-to-one
			name="carDriver"
			class="com.gdssoft.oa.model.admin.CarDriver"
            not-null="false"
			fetch="select"
		>	
			<column name="DRIVER_ID" />
		</many-to-one>
    <!-- imported bi-directional many-to-one association to CarOilCard 
		<many-to-one
			name="carOilCard"
			class="com.gdssoft.oa.model.admin.CarOilCard"
            not-null="false"
			fetch="select"
		>	
			<column name="OIL_CARD_ID" />
		</many-to-one>
		-->
    <!-- imported bi-directional many-to-one association to CarPassFeeCard 
		<many-to-one
			name="carPassFeeCard"
			class="com.gdssoft.oa.model.admin.CarPassFeeCard"
            not-null="false"
			fetch="select"
		>	
			<column name="PAY_CARD_ID" />
		</many-to-one>
		-->
    </class>
</hibernate-mapping>
