<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.admin.CarDriver"
        table="car_driver"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="19"
        >
              <generator class="native"/>
        </id>
         <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CODE"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="NAME"
            length="50"
            not-null="true"
            unique="false"
        />
        <property
            name="cellPhone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CELL_PHONE"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="shortPhone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SHORT_PHONE"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="homePhone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="HOME_PHONE"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="email"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="EMAIL"
            length="150"
            not-null="false"
            unique="false"
        />
        <property
            name="isLeaving"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="IS_LEAVING"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="imageSource"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="IMAGE_SOURCE"
            length="150"
            not-null="false"
            unique="false"
        />
        <property
            name="licenseClass"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="LICENSE_CLASS"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="licenseSDt"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="LICENSE_S_DT"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="licenseEDt"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="LICENSE_E_DT"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CREATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
         <property
            name="status"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="STATUS"
            length="50"
            not-null="false"
            unique="false"
        />
  <!-- exported bi-directional one-to-many association to CarUsing -->
	<set name="carUsings"
	     table="car_using"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="DRIVER_ID"/>
		</key>
		<one-to-many
			class="com.gdssoft.oa.model.admin.CarUsing"
		/>
	</set>     
    </class>
</hibernate-mapping>
