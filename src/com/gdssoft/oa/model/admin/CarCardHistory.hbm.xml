<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.admin.CarCardHistory"
        table="car_card_history"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="ID"
            type="java.lang.Long"
            length="19"
        >
              <generator class="native"/>
        </id>
         <property
            name="cardType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="CARD_TYPE"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="cardId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="CARD_ID"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="useType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="USE_TYPE"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="amount"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="AMOUNT"
            precision="18"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="useDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="USE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DESCRIPTION"
            length="400"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CREATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="23"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_BY"
            length="50"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
