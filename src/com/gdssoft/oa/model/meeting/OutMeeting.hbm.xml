<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.gdssoft.oa.model.meeting.OutMeeting"
        table="OUT_MEETING"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="meetingId"
            column="MEETING_ID"
            type="java.lang.Long"
            length="19"
        >
              <generator class="native"/>
        </id>
         <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="NAME"
            length="512"
            not-null="true"
            unique="false"
        />
        <property
            name="host"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="HOST"
            length="40"
            not-null="false"
            unique="false"
        />
        <property
            name="recorder"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="RECORDER"
            length="40"
            not-null="true"
            unique="false"
        />
        <property
            name="recordTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="RECORD_TIME"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="contactPerson"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CONTACT_PERSON"
            length="40"
            not-null="false"
            unique="false"
        />
        <property
            name="contactTel"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CONTACT_TEL"
            length="40"
            not-null="false"
            unique="false"
        />
        <property
            name="holdTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="HOLD_TIME"
            length="7"
            not-null="true"
            unique="false"
        />
        <property
            name="holdLocation"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="HOLD_LOCATION"
            length="512"
            not-null="false"
            unique="false"
        />
        <property
            name="attendType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="ATTEND_TYPE"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="attendLeaders"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ATTEND_LEADERS"
            length="512"
            not-null="false"
            unique="false"
        />
         <property
            name="attendOfficers"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ATTEND_OFFICERS"
            length="512"
            not-null="false"
            unique="false"
        />
        <property
            name="attendOfficeLeaders"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ATTEND_OFFICE_LEADERS"
            length="512"
            not-null="false"
            unique="false"
        />
        <property
            name="attendSubDeps"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ATTEND_SUB_DEPS"
            length="512"
            not-null="false"
            unique="false"
        />
        <property
            name="holdDep"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="HOLD_DEP"
            length="512"
            not-null="false"
            unique="false"
        />
        <property
            name="shortDesc"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SHORT_DESC"
            length="1024"
            not-null="false"
            unique="false"
        />
         <property
            name="runId"
            type="java.lang.Long"
            column="RUN_ID"
        />
     <set name="docs"
	     table="out_meeting_docs"
	     lazy="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="MEETINGID"/>
		</key>
		<many-to-many column="fileId"
			class="com.gdssoft.oa.model.system.FileAttach"
		/>
	</set>   
    </class>
</hibernate-mapping>
